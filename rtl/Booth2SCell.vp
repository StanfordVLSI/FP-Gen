//; # Perl Libraries
//;
//;



module `mname` (
    input logic [2:0] y,
    input logic [1:0] sel_x,
    output logic S, xn0           
		);

  assign xn0 = ~y[2];
  assign S = y[2] & ~( sel_x[1] & sel_x[0] );

   
endmodule : `mname`
